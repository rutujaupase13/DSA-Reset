import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the value:");
        int num = sc.nextInt();
        int sum = 0;
        sum = (num * (num + 1) / 2);
        System.out.println("Sum is "+sum);
    }
}
